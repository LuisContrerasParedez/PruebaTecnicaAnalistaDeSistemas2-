
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model Accion {
  Id            Int       @id @default(autoincrement())
  Nombre        String    @db.NVarChar(100)
  Descripcion   String?   @db.NVarChar(255)
  FechaCreacion DateTime  @default(now()) @db.DateTime2

  @@map("Acciones") 
}